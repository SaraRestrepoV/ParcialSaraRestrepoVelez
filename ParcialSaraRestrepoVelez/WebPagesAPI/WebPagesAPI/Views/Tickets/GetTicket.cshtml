@model ParcialSaraRestrepoVelez.DAL.Entities.Ticket

@{
    ViewData["Title"] = "GetTicket";
}


<h4>Validar Ticket</h4>

<div class="row">
    <div class="col-md-4">
        <form asp-controller="Ticket" method="get" asp-action="edit" >
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>

            <div class="form-group">
                <label class="control-label">Ticket Id</label>
                <input name="id" class="form-control" />
                <span asp-validation-for="Id" class="text-danger"></span>
            </div>

            <div class="form-group p-4">
                <input type="submit" value="Consultar" class="btn btn-outline-primary" />
                <a asp-action="GetTicket" class="btn btn-outline-success">Regresar</a>
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
